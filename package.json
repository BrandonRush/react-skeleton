{
  "name": "react-skeleton",
  "version": "0.0.0",
  "private": true,
  "description": "A lightweight React project skeleton.",
  "author": "Brandon Rush",
  "license": "MIT",
  "engines": {
    "node": ">=6.13.1",
    "npm": ">=3.10.10"
  },
  "dependencies": {
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "normalize.css": "^8.0.0",
    "passport": "^0.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.0",
    "jest": "^23.1.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.16.5"
  },
  "scripts": {
    "precommit": "lint-staged",
    "flow": "flow",
    "flow:check": "flow check",
    "test": "jest",
