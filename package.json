  "scripts": {
    "build:dev": "yarn run clean && webpack --config webpack.dev.js",
    "build:prod": "yarn run clean && webpack --config webpack.prod.js",
    "clean": "rimraf ./build",
    "docker:run": "docker run -d \"react-skeleton\"",
    "docker:build": "docker build -t \"react-skeleton\" .",
    "serve": "yarn run build:prod && nodemon ./server.js",
    "serve:inspect": "yarn run build:prod && nodemon --inspect ./server.js",
    "start:dev": "webpack-dev-server --config webpack.dev.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest"
  }
